all:
	gcc -O0 -g3 -march=skylake -o with_vectorization_o0 ./with_vectorization.cpp
	gcc -O3 -g3 -march=skylake -o with_vectorization_o3 ./with_vectorization.cpp
	gcc -O0 -g3 -march=skylake -o without_vectorization_o0 ./without_vectorization.cpp
	gcc -O3 -g3 -march=skylake -o without_vectorization_o3 ./without_vectorization.cpp
